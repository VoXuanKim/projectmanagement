    var storedProjectId = localStorage.getItem('selectedProjectId');
    var storedRole = localStorage.getItem('selectedRole');

    // S·ª≠ d·ª•ng th√¥ng tin n·∫øu c√≥
    if (storedProjectId && storedRole) {
