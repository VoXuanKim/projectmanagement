        // console.log('Stored Role:', storedRole);

        // C·∫≠p nh·∫≠t URL c·ªßa trang v·ªõi th√¥ng tin t·ª´ local storage
        var currentUrl = window.location.href;
        window.history.replaceState({}, document.title, currentUrl);
